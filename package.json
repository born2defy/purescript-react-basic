  "devDependencies": {
    "babel-core": "^6.26.3",
    "bower": "^1.8.4",
    "parcel-bundler": "^1.10.3",
    "pulp": "^12.2.0",
    "purescript": "^0.12.0",
    "purescript-psa": "^0.7.3",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0"
  }
