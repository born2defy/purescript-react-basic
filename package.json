{
  "name": "purescript-react-basic",
  "version": "0.0.0",
  "description": "Dev deps for purescript-react-basic",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "pulp build",
    "build:examples": "PATH=`echo $PATH | sed -e 's/:\\.\\/node_modules\\/\\.bin//'` find examples -maxdepth 2 -type f -iname makefile -execdir make \\;",
